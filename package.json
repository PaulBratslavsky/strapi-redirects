{
  "name": "strapi-plugin-redirects",
  "version": "1.0.4",
  "description": "This plugin allows a user to manage redirects from the admin panel.",
  "license": "MIT",
  "author": {
    "name": "Alex Bennett",
    "email": "info@alex-bennett.co.uk",
    "url": "https://alex-bennett.co.uk"
  },
  "maintainers": [
    {
      "name": "Alex Bennett"
    }
  ],

  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "source": "./admin/src/index.js",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./server/src/index.js",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "./strapi-server.js"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "verify": "strapi-plugin verify",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link"
  },

  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.9",
    "@strapi/icons": "^2.0.0-rc.9",
    "react-intl": "^6.6.8",
    "csvtojson": "^2.0.10",
    "lodash": "4.17.21",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.1.0",
    "@strapi/strapi": "^5.0.0-rc.11",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "styled-components": "^6.1.12"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.1.0",
    "@strapi/strapi": "^5.0.0-rc.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "styled-components": "^6.1.12"
  },

  "strapi": {
    "kind": "plugin",
    "name": "redirects",
    "displayName": "Redirects",
    "description": "This plugin allows a user to manage redirects from the admin panel."
  }
}
